/* component/calculate/calc.wxss */
.calc {
    display: flex;
    position: fixed;
    bottom: 0;
    width: 100%;
    /* height: 100px; */
    
}
.calc .number{
    flex:3;
    display:flex;
    flex-flow: wrap;
    background: rgba(0, 0,0,0.05);
    
}
.calc .number text {
    /* flex:1; */
    width:33.3%;
    height: 50px;
    line-height: 50px;
    text-align: center;
}

.calc .other{
    flex:1;
    background: rgba(0, 0,0,0.05);
}
.calc .other view {
    line-height: 50px;
    text-align: center;
    height: 50px;
    /* background: rgba(0, 0,0,0.05); */
}